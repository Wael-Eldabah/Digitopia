{
    "name":  "eyeguard-frontend",
    "version":  "1.0.0",
    "private":  true,
    "description":  "Software-only simulation UI for EyeGuard SOC platform.",
    "scripts":  {
                    "dev":  "vite",
                    "build":  "vite build",
                    "preview":  "vite preview",
                    "test":  "jest",
                    "test:watch":  "jest --watch",
                    "lint":  "eslint src --ext .js,.jsx",
                    "cypress":  "cypress open"
                },
    "dependencies":  {
                         "@tanstack/react-query":  "^4.38.0",
                         "axios":  "^1.7.0",
                         "cytoscape":  "^3.27.0",
                         "react":  "^18.3.1",
                         "react-dom":  "^18.3.1",
                         "react-router-dom":  "^6.23.0"
                     },
    "devDependencies":  {
                            "@babel/preset-env":  "^7.28.3",
                            "@babel/preset-react":  "^7.27.1",
                            "@swc/jest":  "^0.2.39",
                            "@testing-library/jest-dom":  "^6.4.1",
                            "@testing-library/react":  "^14.2.1",
                            "@testing-library/user-event":  "^14.5.1",
                            "@types/jest":  "^29.5.12",
                            "@vitejs/plugin-react":  "^4.2.1",
                            "autoprefixer":  "^10.4.21",
                            "babel-jest":  "^29.7.0",
                            "cypress":  "^13.6.4",
                            "eslint":  "^8.56.0",
                            "eslint-config-prettier":  "^9.1.0",
                            "eslint-plugin-react":  "^7.33.2",
                            "identity-obj-proxy":  "^3.0.0",
                            "jest":  "^29.7.0",
                            "jest-environment-jsdom":  "^29.7.0",
                            "postcss":  "^8.4.35",
                            "tailwindcss":  "^3.4.7",
                            "vite":  "^5.1.4"
                        },
    "jest":  {
                 "testEnvironment":  "jsdom",
                 "setupFilesAfterEnv":  [
                                            "\u003crootDir\u003e/setupTests.js"
                                        ],
                 "moduleNameMapper":  {
                                          "^.+\\.(css|scss|less)$":  "identity-obj-proxy"
                                      },
                 "roots":  [
                               "\u003crootDir\u003e/src",
                               "\u003crootDir\u003e/../tests/frontend"
                           ],
                 "transform":  {
                                   "^.+\\\\.[tj]sx?$":  [
                                                            "@swc/jest",
                                                            {
                                                                "jsc":  {
                                                                            "parser":  {
                                                                                           "jsx":  true,
                                                                                           "syntax":  "ecmascript"
                                                                                       },
                                                                            "transform":  {
                                                                                              "react":  {
                                                                                                            "runtime":  "automatic"
                                                                                                        }
                                                                                          }
                                                                        }
                                                            }
                                                        ]
                               }
             }
}
